<p id="notice"><%= notice %></p>

<h1>Listing Calender Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calender_events.each do |calender_event| %>
      <tr>
        <td><%= link_to 'Show', calender_event %></td>
        <td><%= link_to 'Edit', edit_calender_event_path(calender_event) %></td>
        <td><%= link_to 'Destroy', calender_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calender event', new_calender_event_path %>
