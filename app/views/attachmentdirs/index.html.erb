<p id="notice"><%= notice %></p>

<h1>Listing Attachmentdirs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attachmentdirs.each do |attachmentdir| %>
      <tr>
        <td><%= link_to 'Show', attachmentdir %></td>
        <td><%= link_to 'Edit', edit_attachmentdir_path(attachmentdir) %></td>
        <td><%= link_to 'Destroy', attachmentdir, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attachmentdir', new_attachmentdir_path %>
