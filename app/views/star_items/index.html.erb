<p id="notice"><%= notice %></p>

<h1>Listing Star Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @star_items.each do |star_item| %>
      <tr>
        <td><%= link_to 'Show', star_item %></td>
        <td><%= link_to 'Edit', edit_star_item_path(star_item) %></td>
        <td><%= link_to 'Destroy', star_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Star item', new_star_item_path %>
